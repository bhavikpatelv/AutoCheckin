        <TextBox x:Name="firstNameTextBox" HorizontalAlignment="Left" Height="23" Margin="98,107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239"/>
        <Label x:Name="label2" Content="Address" HorizontalAlignment="Left" Margin="26,135,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="addressTextBox" HorizontalAlignment="Left" Height="23" Margin="98,139,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239"/>
        <Label x:Name="label3" Content="State" HorizontalAlignment="Left" Margin="26,199,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="stateTextBox" HorizontalAlignment="Left" Height="23" Margin="98,203,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239"/>
        <Label x:Name="label4" Content="Zip Code" HorizontalAlignment="Left" Margin="26,231,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="zipCodeTextBox" HorizontalAlignment="Left" Height="23" Margin="98,235,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239"/>

    </Grid>
</Window>
