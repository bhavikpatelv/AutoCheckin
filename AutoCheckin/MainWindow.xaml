<Window x:Class="AutoCheckin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoCheckin"
        mc:Ignorable="d"
        Title="MainWindow" Height="486.069" Width="729.103">
    <Grid>
        <Button x:Name="Load_Image" Content="Load" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="408,401,0,0" Click="button_Click"/>
        <Image x:Name="image1" HorizontalAlignment="Left" Height="312" Margin="479,10,0,0" VerticalAlignment="Top" Width="227"/>
        <Label x:Name="Label" Content="Last Name" HorizontalAlignment="Left" Margin="26,72,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.043,0.057"/>
        <TextBox x:Name="lastNameTextBox" HorizontalAlignment="Left" Height="22" Margin="98,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239"/>
        <Label x:Name="label" Content="City" HorizontalAlignment="Left" Margin="26,167,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="cityTextBox" HorizontalAlignment="Left" Height="23" Margin="98,171,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239"/>
